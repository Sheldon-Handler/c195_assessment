<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" animated="false" collapsible="false"
            text="%add.customer" xmlns="http://javafx.com/javafx/17.0.1"
            fx:controller="com.example.c195_assessment.controller.AddCustomerController">
    <content>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints minWidth="150.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="250.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="%customer.id" GridPane.rowIndex="1">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="customerId" cacheShape="false" disable="true" editable="false" layoutX="171.0"
                           layoutY="83.0" promptText="%auto.generated" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="%customer.name" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="customerName" cacheShape="false" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="%address" GridPane.rowIndex="3">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="address" cacheShape="false" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Label layoutX="21.0" layoutY="116.0" text="%city" GridPane.rowIndex="4">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="city" cacheShape="false" layoutX="171.0" layoutY="113.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="4"/>
                <Label text="%postal" GridPane.rowIndex="5">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="postal" cacheShape="false" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <Label text="%phone" GridPane.rowIndex="6">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="phone" cacheShape="false" layoutX="197.0" layoutY="83.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="6"/>
                <Label text="%country" GridPane.rowIndex="7">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <ChoiceBox fx:id="country" prefHeight="27.0" prefWidth="249.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="7"/>
                <Label text="%division" GridPane.rowIndex="8">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <ChoiceBox fx:id="division" cacheShape="false" disable="true" prefHeight="27.0" prefWidth="249.0"
                           GridPane.columnIndex="1" GridPane.rowIndex="8"/>
                <ButtonBar cacheShape="false" GridPane.columnIndex="1" GridPane.rowIndex="10">
                    <buttons>
                        <Button fx:id="saveButton" defaultButton="true" mnemonicParsing="false"
                                onAction="#onSaveButtonAction" text="%save"/>
                        <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false"
                                onAction="#onCancelButtonAction" text="%cancel"/>
                    </buttons>
                </ButtonBar>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </GridPane>
    </content>
    <font>
        <Font name="System Bold" size="14.0"/>
    </font>
</TitledPane>
